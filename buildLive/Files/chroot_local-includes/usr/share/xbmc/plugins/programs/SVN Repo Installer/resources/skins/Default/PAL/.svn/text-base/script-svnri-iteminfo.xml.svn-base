				<onright>20</onright>
				<ondown>20</ondown>
				<onup>20</onup>
				<showonepage>false</showonepage>
				<orientation>vertical</orientation>
				<visible>Control.IsVisible(10)</visible>
			</control>
			<control type="button">
				<posx>0r</posx>
				<posy>215</posy>
				<width>10</width>
