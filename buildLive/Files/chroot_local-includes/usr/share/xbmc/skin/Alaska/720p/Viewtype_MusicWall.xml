		<control type="panel" id="50">
			<viewtype label="Wall">icon</viewtype>
			<posx>373</posx>
			<posy>225</posy>
			<width>540</width>
			<height>540</height>
			<pagecontrol>61</pagecontrol>
			<scrolltime>0</scrolltime>
			<orientation>horizontal</orientation>
			<onup>7000</onup>
			<ondown>7000</ondown>
			<preloaditems>2</preloaditems>
			<include>Animation_ZoomInZoomOut</include>
			<include>Animation_MusicWallFloat</include>
			<include>Animation_ZoomedByMenu</include>
			<include>Animation_HiddenByMusicInfo</include>
			<itemlayout width="180" height="180">
				<control type="image">
					<posx>7</posx>
					<posy>7</posy>
					<width>160</width>
					<height>160</height>
					<texture>wall/musicwallmatte.png</texture>
					<colordiffuse>WallIconDiffuse</colordiffuse>
				</control>
				<control type="image">
					<posx>6</posx>
					<posy>6</posy>
					<width>162</width>
					<height>162</height>
					<fadetime>200</fadetime>
					<texture background="true" diffuse="wall/musicwallmatte.png">$INFO[ListItem.Icon]</texture>
					<bordertexture border="2">wall/musicwallframeNF.png</bordertexture>
					<bordersize>1</bordersize>
					<colordiffuse>WallIconDiffuse</colordiffuse>
					<aspectratio stretchdiffuse="false">stretch</aspectratio>
				</control>				
			</itemlayout>
			<focusedlayout width="180" height="180">
